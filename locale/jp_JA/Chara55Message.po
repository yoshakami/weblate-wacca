msgid "【Ais】'<ANNONCER>"
msgstr "【Ais】'<ADVERTISE>"

msgid "Ouais······"
msgstr "うん・・・・・・"

msgid "Ce ton"
msgstr "その調子"

msgid "Tu peux quand même y aller ?"
msgstr "まだいける？"

msgid "N'abandonne pas jusqu'à la fin"
msgstr "最後まで気を抜かないで"

msgid "Oui, battons-nous !"
msgstr "うん、戦おう！"

msgid "Le temps s'est écoulé.... allons-y!"
msgstr "時間切れ・・・・・・。行こう！"

msgid "Eh bien, bonne chance."
msgstr "それじゃあ、頑張ってね。"

msgid "Félicitations"
msgstr "おめでとう"

msgid "Qu'est-ce que c'est...?"
msgstr "なんだろう・・・・・・？"

msgid "Ouais, bien."
msgstr "うん、よかった。"

msgid "mission······.\r\nEst-ce comme une quête ?"
msgstr "ミッション・・・・・・。\r\nクエストみたいなこと？"

msgid "C'est dommage..."
msgstr "残念だけど・・・・・・"

msgid "Jeu unique !"
msgstr "シングルプレイ！"

msgid "Multijoueur !"
msgstr "マルチプレイ！"

msgid "Mettez-vous en scène !"
msgstr "ステージアップ！"

msgid "boutique!"
msgstr "ショップ！"

msgid "Choisissez ensuite"
msgstr "じゃあ、選んで"

msgid "Maintenant, choisissez une chanson à jouer."
msgstr "じゃあ、プレイする曲を選んでね"

msgid "Alors, choisissez une chanson à jouer.\r\nC'est la dernière chanson."
msgstr "じゃあ、プレイする曲を選んで。\r\n次が最後の曲だよ。"

msgid "Attention au temps restant..."
msgstr "残り時間、気をつけて・・・・・・"

msgid "On dirait qu'il fait quelque chose. On y va ?"
msgstr "何かやってるみたい。行ってみる？"

msgid "Clair!"
msgstr "クリア！"

msgid "MissMoins ! !"
msgstr "MissLess！！"

msgid "Combinaison complète ! !"
msgstr "FullCombo！！"

msgid "Tout merveilleux ! !"
msgstr "All Marvelous！！"

msgid "Est-ce que vous plaisantez...?"
msgstr "ふざけてるの・・・・・・？"

msgid "Je suppose que ça va...?"
msgstr "まあまあ・・・・・・なのかな？"

msgid "Je suppose que ça va... ?"
msgstr "まあまあ・・・・・・なのかな？"

msgid "Ouais. Je pense que c'est du bon travail..."
msgstr "うん。上出来・・・・・・だと思う。"

msgid "Tu es incroyable...\r\nComment peux-tu le faire si bien ?"
msgstr "君はすごいね・・・・・・。\r\nどうしてそんなにうまくできるの？"

msgid "Êtes-vous d'accord?"
msgstr "大丈夫？"

msgid "Classé...\r\nVous avez travaillé très dur."
msgstr "ランキング入り・・・・・・。\r\nすごく頑張ったんだね。"

msgid "Je me suis bien amusé aussi. merci."
msgstr "私も楽しかったよ。ありがとう。"

msgid "Pouvez-vous revenir ? Je suis en attente."
msgstr "また来られる？待ってる。"

msgid "WACCA.......\r\nPuis-je le toucher aussi... ?"
msgstr "WACCA・・・・・・、\r\n私も触ってみてもいい・・・・・・？"

msgid "Vos résultats cette fois."
msgstr "今回の、君の成績。"

msgid "Ravi de vous rencontrer"
msgstr "よろしくね"

msgid "Bonne chance aujourd'hui également."
msgstr "今日も、頑張ってね。"

msgid "Compris. Eh bien, allons-y ensemble."
msgstr "わかった。それじゃあ、一緒に行こう。"

msgid "Bonus de connexion !"
msgstr "ログインボーナス！"

msgid "Puis-je faire quelque chose pour vous aider ?"
msgstr "なにか手伝えること、ある？"

msgid "······Ouais. merci."
msgstr "・・・・・・うん。よろしく、お願いします。"

msgid "On fait une sieste ensemble ?"
msgstr "一緒にお昼寝、しようか。"

msgid "Continuer...?"
msgstr "コンティニュー、する・・・・・・？"

msgid "Dois-je vous le dire ?"
msgstr "私が教えてあげようか？"
